.i-tag {
  display: inline-block;
  height: 18px;
  line-height: 18px;
  padding: 0 4px;
  border-radius: 2px;
  background: #fff;
  font-size: 11px;
  vertical-align: middle;
  border: 1rpx solid #dddee1;
}

.i-tag-none {
  border-color: #fff;
}

.i-tag-default {
  border-color: #dddee1;
  background: #e9eaec;
}

.i-tag-red {
  background: #ed3f14;
  color: #fff;
}

.i-tag-red-border {
  color: #ed3f14;
  background: #fff;
  border-color: #ed3f14;
}

.i-tag-red-checked {
  background: #ed3f14;
  color: #fff;
  border-color: #ed3f14;
}

/*自定义颜色*/

.i-tag-reds {
  color: #ed3f14;
}

.i-tag-reds-border {
  color: #000;
}

.i-tag-reds-checked {
  color: #ed3f14;
  border: none !important;
  background-color: transparent !important;
  opacity: 0.9;
}

